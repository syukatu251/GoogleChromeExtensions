{
  "name": "ClipPage",
  "version": "1.0",
  "description": "save local file with npapi",
  "browser_action": {
    "default_icon": "icon.png",
    "popup": "popup.htm"
  },
  "background": {
    "page": "background.htm"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
	  "css": [
			"js/libs/development-bundle/themes/base/jquery.ui.core.css",
			"js/libs/development-bundle/themes/base/jquery.ui.resizable.css",
			"js/libs/development-bundle/themes/base/jquery.ui.theme.css"
		],
      "js": [
			"js/libs/jquery-1.7.2.js",
			"js/libs/development-bundle/ui/jquery.ui.core.js",
			"js/libs/development-bundle/ui/jquery.ui.widget.js",
			"js/libs/development-bundle/ui/jquery.ui.mouse.js",
			"js/libs/development-bundle/ui/jquery.ui.draggable.js",
			"js/libs/development-bundle/ui/jquery.ui.resizable.js",
			"js/content_scripts/EwpscChrome.js", 
			"js/content_scripts/EwpscBackground.js", 
			"js/content_scripts/EwpscWebPage.js", 
			"js/content_scripts/EwpscContentScripts.js"
		]
    }
  ],
  "plugins": [
    { "path": "npEWebPrint.dll" }
  ]
}